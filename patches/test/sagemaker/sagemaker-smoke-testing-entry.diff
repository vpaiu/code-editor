+	
+	// Wait for clone to complete
+	await page.waitForTimeout(10000);
+	
+	// Install npm dependencies for the test project
+	await page.keyboard.type('cd /home/sagemaker-user/vscode-smoketest-express && npm install');
+	await page.keyboard.press('Enter');
